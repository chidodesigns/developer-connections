/div>\n  );\n};\n\nCommentItem.propTypes = {\n  postId: PropTypes.string.isRequired,\n  comment: PropTypes.object.isRequired,\n  auth: PropTypes.object.isRequired,\n  deleteComment: PropTypes.func.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  auth: state.auth,\n});\n\nexport default connect(mapStateToProps, {deleteComment})(CommentItem);\n","/Users/chidozieukaigwe/Projects/nodejs-projects/dev-connector/client/src/components/layout/NotFound.js",[],"/Users/chidozieukaigwe/Projects/nodejs-projects/dev-connector/client/src/components/routing/Routes.js",[],{"ruleId":"209","replacedBy":"210"},{"ruleId":"211","replacedBy":"212"},{"ruleId":"213","severity":1,"message":"214","line":11,"column":1,"nodeType":"215","endLine":54,"endColumn":2},{"ruleId":"213","severity":1,"message":"216","line":10,"column":1,"nodeType":"215","endLine":46,"endColumn":2},{"ruleId":"217","severity":1,"message":"218","line":41,"column":5,"nodeType":"219","endLine":41,"endColumn":33,"suggestions":"220"},{"ruleId":"217","severity":1,"message":"221","line":11,"column":7,"nodeType":"219","endLine":11,"endColumn":23,"suggestions":"222"},{"ruleId":"213","severity":1,"message":"214","line":19,"column":1,"nodeType":"215","endLine":81,"endColumn":2},{"ruleId":"209","replacedBy":"223"},{"ruleId":"211","replacedBy":"224"},{"ruleId":"225","severity":1,"message":"226","line":98,"column":15,"nodeType":"227","messageId":"228","endLine":98,"endColumn":18},{"ruleId":"225","severity":1,"message":"226","line":175,"column":15,"nodeType":"227","messageId":"228","endLine":175,"endColumn":18},{"ruleId":"217","severity":1,"message":"229","line":14,"column":8,"nodeType":"219","endLine":14,"endColumn":17,"suggestions":"230"},{"ruleId":"225","severity":1,"message":"231","line":1,"column":17,"nodeType":"227","messageId":"228","endLine":1,"endColumn":25},"no-native-reassign",["232"],"no-negated-in-lhs",["233"],"import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDecl